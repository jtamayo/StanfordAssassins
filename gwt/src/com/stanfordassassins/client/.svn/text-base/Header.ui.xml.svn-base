<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		/* Add CSS here. See the GWT docs on UI Binder for more details */
		.main {
			color: white;
			font-size: 26px;
			padding-left: 15px;
			padding-top: 10px;
			padding-bottom: 10px;
		}
		
		.right {
			color: white;
			font-size: small;
			float: right;
		}
		.logout {
			cursor: inherit;
		}
	</ui:style>
	
	<g:HTMLPanel styleName="headerCont">
		<div class="{style.right}">
			<g:InlineLabel ui:field="nameLabel"/> | 
			<g:Anchor ui:field="logOutAnchor" addStyleNames="{style.logout}" href="#" text="Log out"/>
		</div>
		<div class="{style.main}">
			<g:Image url="images/logo1.png"></g:Image>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 