/**
 * Different views of the LeaderBoard 
 * 
 * @author andreas
 */
public enum LeaderboardOption {
	GENERAL("General"),
	ASSASSINATIONS("Assassinations"),
	GAMESWON("Games Won"), 
	GAMESPLAYED("Games Played")
	;
	
	private String longName; 
	
	private LeaderboardOption(String longName) { 
	       this.longName = longName; 
	} 
	public String toString () { 
		return this.longName; 
	}
}