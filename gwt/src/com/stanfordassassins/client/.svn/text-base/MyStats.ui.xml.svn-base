<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		/* Add CSS here. See the GWT docs on UI Binder for more details */
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:VerticalPanel>
			<g:HorizontalPanel spacing="5">
			<g:Label>Total Games Played: </g:Label> <g:Label ui:field="gamesPlayed" />
			</g:HorizontalPanel>
			
			<g:HorizontalPanel spacing="5">
			<g:Label>Total games won: </g:Label> <g:Label ui:field="gamesWon" />
			</g:HorizontalPanel>
			
			<g:HorizontalPanel spacing="5">
			<g:Label>So far you've killed: </g:Label> <g:Label ui:field="assassinations" />
			</g:HorizontalPanel>
			
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 