<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app id='/'>
  
  <!-- Servlets -->
   <servlet>
    <servlet-name>proxy</servlet-name>
    <servlet-class>com.stanfordassassins.server.ProxyServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>proxy</servlet-name>
    <url-pattern>/proxy</url-pattern>
  </servlet-mapping>
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>StanfordAssassins.html</welcome-file>
  </welcome-file-list>

</web-app>
